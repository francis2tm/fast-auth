{
  "name": "@fast-auth/react",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts",
      "default": "./src/server.ts"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./src/config.ts",
      "default": "./src/config.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "spec": "cargo run --manifest-path ../../Cargo.toml --bin openapi",
    "generate": "pnpm run spec && openapi-ts",
    "build": "pnpm run generate && tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "tsc --noEmit && eslint .",
    "clean": "git clean -xdf"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.86.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "@types/react": "^19.0.0",
    "eslint": "^9.32.0",
    "typescript": "^5.8.0"
  }
}
