{
  "name": "@fast-auth/sdk",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts",
      "default": "./src/server.ts"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./src/config.ts",
      "default": "./src/config.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "spec": "echo \"Generate docs/openapi.yml from the fast-auth crate.\"",
    "generate": "openapi-ts",
    "build": "pnpm run generate && tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "tsc --noEmit && eslint .",
    "clean": "git clean -xdf"
  },
  "dependencies": {},
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.86.0",
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.32.0",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.40.0"
  }
}
